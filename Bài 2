#include <Arduino.h>

#define LED 2
#define BUTTON 15

void setup() {
    Serial.begin(115200);
    pinMode(LED, OUTPUT);
    pinMode(BUTTON, INPUT_PULLUP);
}

void loop() {
    if (digitalRead(BUTTON) == LOW) {
        Serial.println("Button is Pressed");
        digitalWrite(LED, HIGH);
        delay(500);
    } else {
        digitalWrite(LED, LOW);
    }
}
